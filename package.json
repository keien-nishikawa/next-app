{
  "name": "next-app",
  "version": "0.0.1",
  "repository": "git@github.com:keien-nishikawa/next-app.git",
  "author": "keien-nishikawa",
  "private": false,
  "license": "MIT",
  "scripts": {
    "export": "next export",
    "dev": "next -p 3000",
    "build": "next build",
    "start": "next start",
    "clean:node_modules": "rm -rf ./node_modules & rm package-lock.json & npm cache clean -f",
    "prepare": "husky install",
    "lint": "eslint src . --ext .js,.ts,.tsx --cache",
    "format": "eslint --fix . --ext .js,.ts,.tsx --fix --cache",
    "eslint": "npm run format && npm run lint",
    "typecheck": "tsc"
  },
  "engines": {
    "node": "14"
  },
  "dependencies": {
    "axios": "^0.27.0",
    "next": "latest",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hook-form": "^7.22.3",
    "recoil": "^0.7.0",
    "recoil-persist": "^4.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "husky": "^7.0.0",
    "typescript": "^4.4.4",
    "prettier": "^2.4.1",
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.0"
  }
}
